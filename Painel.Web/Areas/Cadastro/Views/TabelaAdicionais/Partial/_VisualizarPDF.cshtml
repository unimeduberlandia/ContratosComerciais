@{
    ViewBag.Title = "Visualizar Contrato Opcional";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Contrato Plano de Saúde</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/Contents/plugins/bootstrap3/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Contents/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Contents/plugins/Ionicons/css/ionicons.min.css" rel="stylesheet" />
    <link href="~/Contents/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    @section js{
        <script src="~/Contents/plugins/pdfnapagina/jquery-1.8.2.js"></script>
        <script src="~/Contents/plugins/pdfnapagina/jquery-ui-1.9.1.custom.js"></script>
              
    }

</head>
<body>
    <div class="col-lg-12">
        <a>Contrato Adicional</a>
        <br />
        <br />
        @if (@ViewBag.nomeArquivo == "")
        {
            <a>Nenhum arquivo para exibir</a>
        }
        else
        {
            <iframe src="~/Publico/ContratosAdicionais/@ViewBag.nomeArquivo" width="100%" style="min-height:800px;"></iframe>
        }
    </div>
</body>
</html>