             $(document).on("click", "[data-click=adicionar-valor]", function () {
                 var idplano = $("#IDPLANO").val();
                 if (idplano == "") {
                     swal({
                         title: "Erro",
                         text: "Primeiro escolha um plano",
                         type: "warning",
                         onClose: () => {

                         }
                     });

                 }
                 else {
                     $.fancybox({
                         href: '@Url.Action("AdicionarValor", "CAEPF")/' + idplano,
                         type: 'ajax',
