                            if (data != false) {
                                $("#ABRANGENCIA").val(data.abrangencia);
                                $("#ACOMODACAO").val(data.acomodacao);
                                $("#MODALIDADE").val(data.modalidade);
                                $("#todosvalores").load('@Url.Action("PartialValores", "CAEPF")');
                            }

                            $.fancybox.hideLoading();
                        });
                };
