            $(document).on("click", "[data-click=adicionar-opcional]", function () {
                $.fancybox({
                    href: '@Url.Action("AdicionarOpcional", "CAEPF")',
                    type: 'ajax',
                    maxWidth: 600,
                    maxHeight: 270,
                    fitToView: false,
                    width: '100%',
                    height: '100%',
                    autoSize: false,
                    afterClose: function () {
