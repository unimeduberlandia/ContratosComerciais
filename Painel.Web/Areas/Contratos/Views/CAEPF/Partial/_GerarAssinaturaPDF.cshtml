            padding-bottom: 2rem;
        }

        h3 {
            margin-top: 2rem;
        }

        h4 {
            font-size: 10px;
        }

        hr {
            color: #000;
            width: 80%;
            float: left;
        }

        [class*="body"] {
            padding-top: 2rem;
            padding-bottom: 2rem;
        }

        [class*="h3"] {
            margin-top: 2rem;
        }

        [class*="h4"] {
            font-size: 10px;
        }

        [class*="hr"] {
            color: #000;
            width: 80%;
            float: left;
        }

        [class*="txtUnimed"] {
            font-size: 14px;
            margin-bottom: 10px;
            height: 10px;
            font-weight: bold;
        }

        [class*="tituloQualifica"] {
            font-size: 20px;
            margin-bottom: 0px;
            height: 0px;
            font-weight: bold;
        }

        [class*="tiraborda"] {
            border: 2px solid #000000;
