            font-size: 19px;
        }

        [class*="tablenegrito"] {
            font-size: 19px;
            margin-bottom: 10px;
            height: 10px;
            font-weight: bold;
        }

        [class*="table2"] {
            width: 100%;
        }

        [class*="table3"] {
            border-bottom: 1px solid #C1C1BC;
            margin-bottom: 5px;
            border-top: 1px solid #C1C1BC;
        }

        [class*="titulo"] {
            width: 100%;
            margin-bottom: 5px;
        }

        [class*="dependentes"] {
            font-size: 16px;
            width: 1124px;
        }

        [class*="box"] {
            border-radius: 3px;
            background: #ffffff;
            border-top: 2px solid #000000;
            border-left: 2px solid #000000;
            border-right: 2px solid #000000;
            border-bottom: 2px solid #000000;
            width: 99.9%;
